{
  "name": "trae-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "client:dev": "vite --port 5173",
    "client:dev:customer": "vite --config customer-vite.config.ts --port 5174",
    "build": "npx --package typescript tsc -b && npx vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "check": "tsc --noEmit",
    "server:dev": "nodemon",
    "dev": "concurrently \"npm run client:dev\" \"npm run server:dev\"",
    "dev:customer": "concurrently \"npm run client:dev:customer\" \"npm run server:dev\"",
    "dev:all": "concurrently \"npm run client:dev\" \"npm run client:dev:customer\" \"npm run server:dev\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:integration": "vitest run src/tests/integration.test.tsx"
  },
  "dependencies": {
    "@google-cloud/local-auth": "^3.0.1",
    "@stripe/stripe-js": "^8.4.0",
    "@supabase/supabase-js": "^2.39.7",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "google-auth-library": "^10.5.0",
    "googleapis": "^166.0.0",
    "lucide-react": "^0.511.0",
    "moment": "^2.29.4",
    "react": "^18.3.1",
    "react-big-calendar": "^1.8.7",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.3.0",
    "recharts": "^3.4.1",
    "sonner": "^1.4.0",
    "stripe": "^14.25.0",
    "tailwind-merge": "^3.0.2",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.25",
    "@types/node": "^22.19.3",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vercel/node": "^5.3.6",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/ui": "^4.0.9",
    "autoprefixer": "^10.4.21",
    "babel-plugin-react-dev-locator": "^1.0.0",
    "concurrently": "^9.2.0",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "jsdom": "^27.2.0",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vite-plugin-trae-solo-badge": "^1.0.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.9"
  }
}
